    "license": "MIT",
    "homepage": "http://laravel.d3catalyst.com/l4-geoip/",
    "authors": [
        {
            "name": "Ricardo Madrigal",
            "email": "soporte@d3catalyst.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
